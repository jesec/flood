{
  "name": "flood",
  "version": "3.1.0",
  "private": false,
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/jesec/flood.git"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "flood": "./dist/server/bin/start.js"
  },
  "scripts": {
    "build": "npm run build-assets && npm run build-ts",
    "build-assets": "node client/scripts/build.js",
    "build-ts": "tsc-transpile-only -p server/tsconfig.json && cat config.js > dist/config.js && chmod 755 dist/server/bin/start.js",
    "build-docs": "jsdoc -c ./.jsdoc.json",
    "build-i18n": "formatjs compile-folder --ast --format simple client/src/javascript/i18n client/src/javascript/i18n/compiled && formatjs compile-folder --ast --format simple client/src/javascript/i18n/translations client/src/javascript/i18n/compiled",
    "deprecated-warning": "node client/scripts/deprecated-warning.js && sleep 10",
    "format-source": "node scripts/prettier.js formatSource",
    "check-compiled-i18n": "npm run build-i18n && npm run format-source && git diff --quiet client/src/javascript/i18n/compiled",
    "check-source-formatting": "node scripts/prettier.js check",
    "check-types": "tsc",
    "lint": "NODE_ENV=development eslint --max-warnings 0 . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepack": "rm -rf dist && npm run build",
    "start": "node --use_strict dist/server/bin/start.js",
    "start:development": "UPDATED_SCRIPT=start:development:server npm run deprecated-warning && npm run start:development:server",
    "start:development:client": "node client/scripts/start.js",
    "start:development:server": "NODE_ENV=development ts-node-dev --transpile-only server/bin/start.ts",
    "start:production": "UPDATED_SCRIPT=start npm run deprecated-warning && npm start",
    "start:watch": "UPDATED_SCRIPT=start:development:client npm run deprecated-warning && npm run start:development:client"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "bencode": "^2.0.1",
    "body-parser": "^1.18.3",
    "chalk": "^4.1.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "deep-equal": "^2.0.3",
    "express": "^4.17.1",
    "feedsub": "^0.7.1",
    "fs-extra": "^9.0.1",
    "geoip-country": "^4.0.36",
    "http-errors": "^1.8.0",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.20",
    "moment": "^2.28.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mv": "^2.1.1",
